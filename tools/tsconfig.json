{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "lib": ["es2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "target": "es5",
    "removeComments": true,
    "types": [
      "node"
    ],
    "baseUrl": "."
  }
}